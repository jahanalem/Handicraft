        <Product id="4234" key="609baf0e-ff6a-4fa2-b253-08dcaf5ef2f7" parentID="3178" level="4" creatorID="2" sortOrder="5" createDate="2017-07-05T13:53:08" updateDate="2017-10-16T18:58:23" nodeName="Double Chocolate Chip Cookies" urlName="double-chocolate-chip-cookies" path="-1,1075,1082,3178,4234" isDoc="" nodeType="1067" creatorName="cake.art@yahoo.com" writerName="Jahan Alem" writerID="0" template="1066" nodeTypeAlias="Product">
