        <Product id="4235" key="c1e4b1d7-a484-48f8-8dea-45859a970245" parentID="3176" level="4" creatorID="2" sortOrder="4" createDate="2017-07-25T12:14:15" updateDate="2017-10-16T18:58:23" nodeName="Dark Chocolate Mississippi Mud Pie mit Dark Chocolate Fudge" urlName="dark-chocolate-mississippi-mud-pie-mit-dark-chocolate-fudge" path="-1,1075,1082,3176,4235" isDoc="" nodeType="1067" creatorName="cake.art@yahoo.com" writerName="Jahan Alem" writerID="0" template="1066" nodeTypeAlias="Product">
          <UmbracoNaviHide>0</UmbracoNaviHide>
